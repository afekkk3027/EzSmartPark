<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/carback5"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin">

    <Button
        android:id="@+id/captureimage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/checkactivatebutton"
        android:layout_marginEnd="30dp"
        android:background="@color/lime"
        android:text="Capture"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/statustextviewss"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/checkactivatebutton"
        android:layout_alignStart="@+id/textView30"
        android:layout_marginBottom="16dp"
        android:text="Status :"
        android:textColor="@color/common_google_signin_btn_text_dark_default" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView21"
        android:layout_below="@+id/textView21"
        android:layout_marginTop="12dp"
        android:text="Location :"
        android:textColor="@color/common_google_signin_btn_text_dark_default" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView19"
        android:layout_below="@+id/textView19"
        android:layout_marginTop="14dp"
        android:text="Park Time :"
        android:textColor="@color/common_google_signin_btn_text_dark_default" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView20"
        android:layout_below="@+id/textView20"
        android:layout_marginTop="18dp"
        android:text="Phone :"
        android:textColor="@color/common_google_signin_btn_text_dark_default" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/checknamessstextview"
        android:layout_below="@+id/checknamessstextview"
        android:layout_marginTop="18dp"
        android:text="Plate Number :"
        android:textColor="@color/common_google_signin_btn_text_dark_default" />

    <TextView
        android:id="@+id/checknamessstextview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/checkplatetext"
        android:layout_below="@+id/checkplatetext"
        android:layout_marginTop="16dp"
        android:text="Name :"
        android:textColor="@color/common_google_signin_btn_text_dark_default" />

    <TextView
        android:id="@+id/checkplatetext"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/checkparktext"
        android:layout_marginEnd="14dp"
        android:layout_marginTop="76dp"
        android:text="Plate Number :"
        android:textAlignment="viewEnd"
        android:textColor="@android:color/background_light" />

    <TextView
        android:id="@+id/checkfilltext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/textView19"
        android:layout_marginTop="90dp"
        android:text="Fill in the detail and click check"
        android:textColor="?attr/colorControlActivated"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/checkparktext"
        android:layout_width="250dp"
        android:layout_height="60dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:text="CHECK PARKING"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="@android:color/holo_green_light"
        android:textSize="30sp" />

    <Button
        android:id="@+id/checkactivatebutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/textView20"
        android:layout_marginBottom="23dp"
        android:background="@color/lime"
        android:text="Check"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/checknametextview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/check_plate_fill"
        android:layout_alignTop="@+id/checknamessstextview"
        android:textColor="@color/yellow" />

    <TextView
        android:id="@+id/checkplatetextview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView19"
        android:layout_alignBottom="@+id/textView19"
        android:layout_alignStart="@+id/checknametextview"
        android:textColor="@color/yellow" />

    <TextView
        android:id="@+id/checktimetextview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView20"
        android:layout_alignBottom="@+id/textView20"
        android:layout_alignStart="@+id/checkplatetextview"
        android:textColor="@color/yellow" />

    <TextView
        android:id="@+id/checkphonetextview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView21"
        android:layout_alignBottom="@+id/textView21"
        android:layout_alignStart="@+id/checktimetextview"
        android:textColor="@color/yellow" />

    <TextView
        android:id="@+id/checklocationtextview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView30"
        android:layout_alignBottom="@+id/textView30"
        android:layout_alignStart="@+id/checkphonetextview"
        android:textColor="@color/yellow" />

    <TextView
        android:id="@+id/checklocationtextview2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/checklocationtextview"
        android:layout_below="@+id/checklocationtextview"
        android:layout_marginTop="18dp"
        android:textColor="@color/yellow" />

    <TextView
        android:id="@+id/checklocationtextview3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/checklocationtextview2"
        android:layout_below="@+id/checklocationtextview2"
        android:layout_marginTop="13dp"
        android:textColor="@color/yellow" />

    <TextView
        android:id="@+id/checkstatustextview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/checklocationtextview3"
        android:layout_alignTop="@+id/statustextviewss"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/datetextview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/checktimetextview"
        android:layout_toEndOf="@+id/checktimetextview"
        android:textColor="@color/yellow" />

    <TextView
        android:id="@+id/check_plate_fill"
        android:layout_width="150dp"
        android:layout_height="30dp"
        android:layout_alignBottom="@+id/checkplatetext"
        android:layout_toEndOf="@+id/checkactivatebutton"
        android:background="@color/common_google_signin_btn_text_dark_default"
        android:text="@string/text_hint_result"
        android:textAlignment="viewStart"
        android:textSize="18sp" />

</RelativeLayout>